{
  "name": "n8n-nodes-sms-reminder",
  "version": "0.1.0",
  "description": "sms reminder api node for n8n ",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Matthieu PETITE",
    "email": "matthieu.petite@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": " https://github.com/matthieupetite/n8n-smsreminder.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "build": "npx rimraf dist && tsc && gulp build:icons",
    "start":"npx nodemon --watch dist --exec \"npx n8n\"",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/SmsReminderApi.credentials.js",
      "dist/credentials/PixabayApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/SmsReminder/SmsReminderGetShortLink.node.js",
      "dist/nodes/SmsReminder/SmsReminderGetAppointments.node.js",
      "dist/nodes/SmsReminder/SmsReminderGetUserToProcess.node.js",
      "dist/nodes/SmsReminder/SmsReminderSendSms.node.js",
      "dist/nodes/SmsReminder/SmsReminderSendSmsReport.node.js",
      "dist/nodes/SmsReminder/SmsReminderGetConfiguration.node.js",
      "dist/nodes/Strapi/StrapiUploadFile.node.js",
      "dist/nodes/Pixabay/PixabayGetImages.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "~8.32.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "n8n": "^1.121.2"
  }
}
